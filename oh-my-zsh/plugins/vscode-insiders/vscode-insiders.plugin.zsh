# VScode zsh plugin
# author: https://github.com/MarsiBarsi

alias vsci='code-insiders .'
alias vscia='code-insiders --add'
alias vscid='code-insiders --diff'
alias vscig='code-insiders --goto'
alias vscin='code-insiders --new-window'
alias vscir='code-insiders --reuse-window'
alias vsciw='code-insiders --wait'
alias vsciu='code-insiders --user-data-dir'

alias vscied='code-insiders --extensions-dir'
alias vsciie='code-insiders --install-extension'
alias vsciue='code-insiders --uninstall-extension'

alias vsciv='code-insiders --verbose'
alias vscil='code-insiders --log'
alias vscide='code-insiders --disable-extensions'
